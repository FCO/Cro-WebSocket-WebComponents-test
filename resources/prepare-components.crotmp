<:sub prepare>
  <script>
    window.ws = new WebSocket(`wss://${location.host}/cmd`);
    window.ws.addEventListener("message", function(event) {
      let data = JSON.parse(event.data);
      console.log(data);
      document.querySelector(`#component-${ data.id }`).innerHTML = data.html
    })
  </script>
</:>
